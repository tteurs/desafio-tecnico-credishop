<% if @proponente.errors.any? %>
  $("#errorsShow").html("<%= escape_javascript render('errors', proponente: @proponente)%>");
<% else %>
  $("#proponenteRow_<%= @proponente.id %>").replaceWith("<%= escape_javascript render('proponente_row', proponente: @proponente)%>");
  $(".close-modal").click();
<% end %>
